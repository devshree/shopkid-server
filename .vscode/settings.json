{
  "editor.fontVariations": false,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 1000,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit",
    "source.fixAll.stylelint": "explicit"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnSaveOptions": {
    "tabSize": 2,
    "insertSpaces": true,
    "trimTrailingWhitespace": true,
    "trimFinalNewlines": true,
    "prettier.disableLanguages": ["html"]
  },
  "go.useLanguageServer": true,
  "go.lintTool": "golangci-lint",
  "go.formatTool": "goimports",
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.snippetSuggestions": "none"
  },
  "gopls": {
    "completionDocumentation": true,
    "completionBudget": "500ms",
    "ui.completion.usePlaceholders": true,
    "ui.diagnostic.analyses": {
      "unusedparams": true,
      "shadow": true
    },
    "ui.semanticTokens": true,
    "formatting.gofumpt": true
  },
  "go.toolsManagement.autoUpdate": true,
  "go.vetOnSave": "workspace",
  "go.toolsManagement.checkForUpdates": "local",

  "terminal.integrated.env.osx": {
    "PATH": "/Users/devshree/.gvm/gos/go1.21.6/bin:/Users/devshree/go/bin:${env:PATH}",
    "GOROOT": "/Users/devshree/.gvm/gos/go1.21.6",
    "GOPATH": "/Users/devshree/go"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.accessibilitySupport": "off",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.automationProfile.osx": {
    "path": "/bin/zsh"
  },
  "explorer.confirmDelete": false,
  "terminal.integrated.enableMultiLinePasteWarning": "auto",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "editor.wrappingStrategy": "advanced",
  "go.alternateTools": {},
  "go.goroot": "~/.gvm/gos/go1.21.6"
}
